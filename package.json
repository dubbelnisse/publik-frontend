{
  "name": "publik-frontend",
  "version": "0.1.0",
  "description": "Frontend for publik.fm",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node lib/index.js",
    "dev": "NODE_ENV=development npm run -s start",
    "lint": "eslint src",
    "test": "./node_modules/.bin/gulp",
    "mochatest": "./node_modules/mocha/bin/mocha --compilers .:src/compiler.js src/**/_tests_/*.spec.js --watch --reporter dot",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Nils Nilsson <nilspontusnilsson@gmail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
